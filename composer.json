{
    "name": "alnaggar/turjuman",
    "description": "Laravel package for route localization.",
    "keywords": [
        "laravel",
        "route",
        "translation",
        "localization"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "أحمد محمد رشاد",
            "email": "ahmed.rashad.alnaggar@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Alnaggar\\Turjuman\\": "src/"
        },
        "files": [
            "helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alnaggar\\Turjuman\\TurjumanServiceProvider"
            ],
            "aliases": {
                "Turjuman": "Alnaggar\\Turjuman\\Turjuman"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}